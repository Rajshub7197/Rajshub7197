package demo;
import java.io.*;

public class FileHandleReadData {
	public static void main(String[] args)throws IOException {
		
		FileReader f4= new FileReader("C:\\Users\\rajsh\\OneDrive\\Desktop\\robo.txt");
		int i;
		while((i=f4.read())!=-1)
		{
			System.out.print((char)i);
		}
		f4.close();
	}

}


//output
hi coders!!!!!! welcome to file handlings 
