package demo;


public class Practice {
	public static void main(String[] args) {
		System.out.println("even no is ");
		int j;
		for (j=1;j<=10;j++) {
			if(j%2==0) {
				System.out.println(j);
			}
		}
		
		
		System.out.println("odd no is : ");
		int i;
		for(i=1;i<=10;i++) {
			if(i%2!=0) {
				System.out.println(i);
			}
			
		}
	}
}
	


